export BROWSER=chromium
export EDITOR=micro

export PATH="$HOME/.local/bin:$PATH"
export PATH="$HOME/bin:$PATH"

export FZF_BASE=/usr/share/fzf
export FZF_DEFAULT_COMMAND='fd'

